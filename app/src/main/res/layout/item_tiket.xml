<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/asal_tujuan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Asal Tujuan" />
    <TextView
        android:id="@+id/jamBerangkat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jam Berangkat" />
    <TextView
        android:id="@+id/jamTiba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jam Tiba" />

    <TextView android:id="@+id/sisaKursi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sisa Kursi"/>

    <TextView
        android:id="@+id/hargaTiket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Harga Tiket" />

</LinearLayout>